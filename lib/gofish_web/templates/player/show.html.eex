<h2>Show Player</h2>

<ul>

  <li>
    <strong>Username:</strong>
    <%= @player.username %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @player.email %>
  </li>

  <li>
    <strong>Rank:</strong>
    <%= @player.rank %>
  </li>

</ul>

<span><%= link "Edit", to: player_path(@conn, :edit, @player) %></span>
<span><%= link "Back", to: player_path(@conn, :index) %></span>
